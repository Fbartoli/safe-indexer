{
  "extends": ["ponder", "prettier"],
  "plugins": ["prettier"],
  "ignorePatterns": ["**/vite.config.ts", "**/dist/**", "**/node_modules/**"],
  "rules": {
    "prettier/prettier": "error",
    "object-curly-spacing": "off",
    "no-trailing-spaces": "off",
    "@typescript-eslint/object-curly-spacing": "off"
  }
}
